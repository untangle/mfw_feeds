+	.type		= TYPE_INT32,
+	.name		= "int",
+	.desc		= "int",
+	.basefmt	= "%d",
+	.size		= 4 * BITS_PER_BYTE,
+	.byteorder	= BYTEORDER_BIG_ENDIAN,
